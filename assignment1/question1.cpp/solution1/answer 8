#include <bits/stdc++.h>
//ONLY INITIAL TWO TEST CONDITION IS CLEAR AFTER THAT ALL ARE FAILED
using namespace std;

int main(){
    int sum=0;
    long a[5];
    //cin>>n;
    for(int i=0;i<5;i++){
        cin>>a[i];
        sum=sum+a[i];
    }
    //cout<<sum;

    int min=a[0];
    int max=0;
    for(int i=0;i<5;i++){
        if(min>a[i]){
            min=a[i];
        }
    }
    //cout<<min;
    for(int i=0;i<5;i++){
        if(max<a[i]){
            max=a[i];
        }
    }
    //cout<<max;
    
    cout<<sum-max<<" "<<sum-min;
}
